{
  "name": "boilerplate-api",
  "version": "1.0.0",
  "scripts": {
    "start": "nest start",
    "test": "nest build && jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "start:dev": "NODE_ENV=development PORT=8080 nest start --watch",
    "start:test": "NODE_ENV=test PORT=8082 nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production PORT=8082 nest start",
    "docker:up": "docker compose -f 'docker-compose.yml' up -d --build",
    "docker:down": "docker compose -f 'docker-compose.yml' down",
    "build": "nest build",
    "nestia": "npx nestia swagger",
    "patch": "ts-patch install && typia patch",
    "db:gen": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:studio": "prisma studio",
    "prepare": "ts-patch install"
  },
  "author": "prpn97",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@nestia/benchmark": "^0.4.3",
    "@nestjs/testing": "^11.0.12",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/nodemailer": "^6.4.17",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "nestia": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "typescript": "~5.8.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.703.0",
    "@nestia/core": "^5.0.0",
    "@nestia/e2e": "^0.8.2",
    "@nestia/fetcher": "^5.0.0",
    "@nestia/sdk": "^5.0.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/config": "^4.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/swagger": "^11.0.6",
    "@prisma/client": "^6.4.1",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.6",
    "express": "^4.21.1",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.11.5",
    "nestjs-pino": "^4.3.1",
    "nodemailer": "^6.9.16",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.4.1",
    "prom-client": "^15.1.3",
    "typia": "^8.0.0",
    "uuidv4": "^6.2.13",
    "zod": "^3.24.2"
  },
  "prisma": {
    "schema": "src/core/prisma/schema.prisma"
  }
}
